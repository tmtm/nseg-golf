<!DOCTYPE html>
<html>
<head>
  <title>NsegGolf</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>

<div class="logout">
  <%= session[:user_id] && link_to('Logout', session_path(session[:user_id]), :method=>:delete) %>
</div>

<div class="message">
  <%= flash[:error] %>
</div>

<%= yield %>

</body>
</html>
